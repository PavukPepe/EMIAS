<UserControl x:Class="CardsLIB.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             xmlns:local="clr-namespace:CardsLIB"
             mc:Ignorable="d" 
             d:DesignHeight="90" d:DesignWidth="125">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <UserControl xmlns:svgc="http://sharpvectors.codeplex.com/svgc/" HorizontalAlignment="Center">
            <svgc:SvgViewbox Source="/Icons/close.svg"/>
        </UserControl>
        <TextBlock Text="Офтальмолог" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16"></TextBlock>
    </Grid>
</UserControl>
